{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "Lite Frontend boilerplate based on Bootstrap 4, with Webpack for assets management and Nunjucks as template engine.",
  "scripts": {
    "build": "rimraf public/build && webpack --mode development --progress",
    "watch": "npm-run-all --parallel watch:build serve",
    "watch:build": "rimraf public/build && webpack --mode development --watch --progress",
    "dev": "npm-run-all --parallel dev:build serve",
    "dev:build": "webpack-dev-server --mode development --hot",
    "serve": "gulp serve",
    "prod": "rimraf public/dist && cross-env NODE_ENV=production webpack --mode production --progress",
    "start": "cross-env NODE_ENV=production node ./bin/www"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Adrien Beaudouin",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "bootstrap": "^4.0.0",
    "bootstrap-vue": "^2.0.0-rc.6",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "file-loader": "^1.1.9",
    "font-awesome": "^4.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "html-loader": "^0.5.5",
    "img-loader": "^2.0.1",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.3",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.20.2",
    "sweetalert2": "^7.18.0",
    "vue": "^2.5.13",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "webpack-notifier": "^1.5.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-livereload": "^0.6.0",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "nunjucks": "^3.1.0",
    "serve-favicon": "^2.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
