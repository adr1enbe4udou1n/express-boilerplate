{
  "extends": "standard",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "plugins": [
    "import",
    "html"
  ],
  "settings": {
    "html/html-extensions": [
      ".vue"
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prefer-arrow-callback": 2
  }
}